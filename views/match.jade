      div.input-wrapper#right-input-wrapper
        input#right-input(type='text')

  div#profile-container
    div#profile-name
    div#rankings
      div#rankings-left
        svg.svg(xmlns='http://www.w3.org/2000/svg', xmlns:xlink='http://www.w3.org/1999/xlink', version='1.1', x='0px', y='0px', viewbox='0 0 32 28', xml:space='preserve', enable-background='new 0 0 32 28')
