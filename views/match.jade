      div#rankings-right
        svg.svg(xmlns='http://www.w3.org/2000/svg', xmlns:xlink='http://www.w3.org/1999/xlink', version='1.1', x='0px', y='0px', width='28px', height='28px', preserveAspectRatio='xMinYMin meet', viewbox='0 0 28 28', xml:space='preserve', enable-background='new 0 0 28 28')
